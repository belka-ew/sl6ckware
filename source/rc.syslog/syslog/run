#!/bin/execlineb -P

###################################
# Start the system logging daemon #
###################################

s6-envdir env
multisubstitute {
  importas -u CMD CMD
  importas -usCd" " CMD_OPTS CMD_OPTS
}
# Using /dev/log socket availability as syslogd readiness indicator
s6-notifyoncheck -dn0 -c "[ -S /dev/log ]"
exec -c $CMD $CMD_OPTS
