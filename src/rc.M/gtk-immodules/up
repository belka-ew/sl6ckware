########################
# Update gtk.immodules #
########################

foreground { echo "Starting oneshot service:  gtk-immodules" }
backtick -ni UPDATE-GTK-IMMODULES { which update-gtk-immodules }
importas -u update-gtk-immodules UPDATE-GTK-IMMODULES
foreground { echo "Updating gtk.immodules:  ${update-gtk-immodules}" }
$update-gtk-immodules
